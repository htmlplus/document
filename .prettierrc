{
  "embeddedLanguageFormatting": "off",
  "printWidth": 120,
  "quoteProps": "consistent",
  "singleQuote": true,
  "trailingComma": "none",
  "importOrder": [
    "^react", 
    "^next", 
    "^@htmlplus/",
    "<THIRD_PARTY_MODULES>", 
    "^@/(.*)$",
    "^\\$",
    "^[.](.*)"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
